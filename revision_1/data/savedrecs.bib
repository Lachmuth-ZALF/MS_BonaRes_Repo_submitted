@article{ WOS:001218628200001,
Author = {Ingwersen, Joachim and Poyda, Arne and Kremer, Pascal and Streck, Thilo},
Title = {Harvest residues: A relevant term in the carbon balance of croplands?},
Journal = {AGRICULTURAL AND FOREST METEOROLOGY},
Year = {2024},
Volume = {349},
Month = {APR 15},
Abstract = {Over the past two decades, major efforts have been made to quantify the
   extent to which and under what conditions croplands are sources or sinks
   for carbon. For this purpose, the net carbon stock change of the study
   site is typically quantified based on net CO 2 fluxes monitored with an
   eddy covariance or chamber system, on measured C import by organic
   fertilizer and C export by harvest. While in cropland studies this
   balance is usually referred to as net biome productivity (NBP), we
   prefer to use the term net ecosystem carbon balance (NECB) here. NECB is
   basically the sum of the stock change of plant carbon ( Delta PC) and
   soil organic carbon ( Delta SOC). In the standard approach, the
   assumption is that at an annual cropland site the bulk of plant biomass
   is typically removed at harvest, and that Delta PC can therefore be
   neglected. In this case, Delta SOC equals NECB. In this paper we show
   that this assumption is problematic, particularly if in crop rotation
   systems the budget is determined over a single cropping period. The
   present contribution extends the concept of NECB to include harvest
   residues (HR) and applies it to a case study over a maize - winter wheat
   - winter wheat rotation at a cropland site in southwest Germany. In all
   three cropping periods, the sign of NECB was opposite that of Delta SOC.
   Accordingly, neglecting HR led to an incorrect result concerning the
   question whether the site is a sink or a source for soil carbon. Our
   findings demonstrate that for croplands HR must be included to obtain an
   accurate and meaningful carbon balance.},
